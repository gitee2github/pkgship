             },\n                //自定义颜色\n                color: [\"#1ab394\", \"#afb4db\", \"#426ab3\", \"#f391a9\"],\n                data: this.roseData\n              }\n            ]\n          })\n        }\n    },\n    computed: {\n        rosecharts(){\n            return 'rosecharts' + Math.random()*5000\n        }\n    },\n    mounted () {\n        this.$nextTick(function() {\n        this.drawPie();\n      });\n    }\n}\n</script>\n\n<style scoped>\n  .container{\n      width: 800px;\n      height: 500px;\n  }\n</style>"],"sourceRoot":"src/components"}]}